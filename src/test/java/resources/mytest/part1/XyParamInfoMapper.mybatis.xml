<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mytest.part1.XyParamInfoDao">

    <select id="getByPrimaryKey" resultType="mytest.part1.XyParamInfo">
        select * from xy_param_info
        where param_type=#{param_type}
        and param_key=#{param_key}
    </select>

</mapper>